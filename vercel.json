{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/backend/api/index.js"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ]
}